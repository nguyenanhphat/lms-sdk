    "test": "jest ./src",
    "test:coverage": "yarn test --coverage",
    "clean": "rimraf ./build ./lib",
    "ts:check": "tsc -p tsconfig.json --noEmit",
    "predev": "yarn clean",
    "dev": "tsc -p tsconfig.json -w",
